@model Instructor
@{
}
<div class="card" style="width: 18rem;">
    <img src="/Pictures/@Model.Img" class="card-img-top">
    <div class="card-body">
        <h5 class="card-title">Student</h5>
        <p class="card-text">
            <tr>
                <th scope="row">@Model.Id</th>
                <td>@Model.Name</td>
                <td>@Model.Salary</td>
                <td>@Model.Address</td>
                <td>@Model.Department.Name</td>
                <td>@Model.Course.Name</td>
            </tr>
        </p>
        <a href="/Instructor/Index" class="btn btn-primary">Back</a>
    </div>
</div>